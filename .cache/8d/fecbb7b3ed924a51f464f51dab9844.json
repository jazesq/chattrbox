r = setInterval(() => {\n\t\t\t$('[data-time]').each((idx, element) => {\n\t\t\t\tlet $element = $(element);\n\t\t\t\tlet timestamp = new Date().setTime($element.attr('data-time'));\n\t\t\t\tlet ago = moment(timestamp).fromNow();\n\t\t\t\t$element.html(ago);\n\t\t\t});\n\t\t}, 1000);\n\t}\n}"},"lineCount":null}},"error":null,"hash":"66bb38023e94f1dff308ea32c94e1eff","cacheData":{"env":{}}}